{% extends 'base.html.twig' %}

{% block title %}Prerempli index{% endblock %}

{% block body %}
    <h1>Prerempli index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Texte</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for prerempli in preremplis %}
            <tr>
                <td>{{ prerempli.id }}</td>
                <td>{{ prerempli.texte }}</td>
                <td>
                    <a href="{{ path('app_prerempli_show', {'id': prerempli.id}) }}">show</a>
                    <a href="{{ path('app_prerempli_edit', {'id': prerempli.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_prerempli_new') }}">Create new</a>
{% endblock %}
